{
    "manifest_version": 2,
    "name": "FluxThemes",
    "version": "1.0",
    "description": "-",
    "permissions": [
        "theme",
        "tabs"
    ],
    "browser_action": {
        "default_icon": {
            "16": "./icons/icon-16.png",
            "32": "./icons/icon-32.png",
            "48": "./icons/icon-48.png",
            "128": "./icons/icon-128.png"
        }
    },
    "background": {
        "scripts": ["background.js"]
    },
    "icons": {
        "16": "./icons/icon-16.png",
        "32": "./icons/icon-32.png",
        "48": "./icons/icon-48.png",
        "128": "./icons/icon-128.png"
    }
}